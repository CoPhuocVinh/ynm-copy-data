{
  "collection": "@nestjs/schematics",
  "sourceRoot": "services/copy-comments-old/src",
  "compilerOptions": {
    "tsConfigPath": "services/copy-comments-old/tsconfig.json",
    "webpack": true,
    "deleteOutDir": true,
    "plugins": []
  },
  "monorepo": true,
  "projects": {
    "copy-comments-old": {
      "type": "application",
      "root": "services/copy-comments-old",
      "entryFile": "main-consumer",
      "sourceRoot": "services/copy-comments-old/src",
      "compilerOptions": {
        "tsConfigPath": "services/copy-comments-old/tsconfig.json"
      }
    },
    "copy-docs-old": {
      "type": "application",
      "root": "services/copy-docs-old",
      "entryFile": "main-consumer",
      "sourceRoot": "services/copy-docs-old/src",
      "compilerOptions": {
        "tsConfigPath": "services/copy-docs-old/tsconfig.json"
      }
    }
  }
} 