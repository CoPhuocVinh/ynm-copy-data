{
  "name": "ynm-copy-data",
  "version": "1.0.0",
  "description": "Data Copy Service using RabbitMQ",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "run:comments-old": "cd services/copy-comments-old && npx ts-node -r tsconfig-paths/register src/main.ts",
    "run:docs-real-time": "cd services/copy-docs-real-time && npx ts-node -r tsconfig-paths/register src/main.ts",
    "run:comments-real-time": "cd services/copy-comments-real-time && npx ts-node -r tsconfig-paths/register src/main.ts",
    "run:docs-old": "cd services/copy-docs-old && npx ts-node -r tsconfig-paths/register src/main.ts",
    "dev:consumer": "cd services/copy-comments-old && npx ts-node -r tsconfig-paths/register src/main-consumer.ts"
  },
  "keywords": [
    "nestjs",
    "rabbitmq",
    "microservices"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/microservices": "^11.1.1",
    "@nestjs/platform-express": "^11.1.1",
    "@types/amqplib": "^0.10.7",
    "amqplib": "^0.10.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "js-yaml": "^4.1.0",
    "nest-commander": "^3.17.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@types/express": "^5.0.1",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^22.15.18",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
